// +gobra

package slices

/** Bytes **/
func AbsSliceC_Bytes_test() {
	s := make([]byte, 10)
	ghost contents := NewSeq_Bytes(10)
	fold AbsSliceC_Bytes(s, 0, 10, contents)
	// assert false // fails
}

func AbsSlice_Bytes_test() {
	s := make([]byte, 10)
	fold AbsSlice_Bytes(s, 0, 10)
	// assert false // fails
}

/** Any **/
func AbsSliceC_Any_test() {
	s := make([]any, 1)
	var elem interface{} = int(1)
	ghost contents := seq[any]{elem}
	s[0] = elem
	fold AbsSliceC_Any(s, 0, 1, contents)
	// assert false // fails
}

func AbsSlice_Any_test() {
	s := make([]any, 10)
	fold AbsSlice_Any(s, 0, 10)
	// assert false // fails
}

ghost
requires size >= 0
ensures len(res) == size
ensures forall i int :: 0 <= i && i < size ==> res[i] == 0
pure func NewSeq_Bytes(size int) (res seq[byte])