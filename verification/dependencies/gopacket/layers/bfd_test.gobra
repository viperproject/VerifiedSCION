// +gobra

package layers

import "github.com/scionproto/scion/verification/utils/slices"

func BFD_test() {
	ensures slices.AbsSlice_Bytes(s2, 0, len(s2))
	decreases
	outline(
	s2 := []byte{1, 2, 3}
	fold slices.AbsSlice_Bytes(s2, 0, len(s2))
	)
	var b *BFDAuthHeader = &BFDAuthHeader{BFDAuthType(1), BFDAuthKeyID(1), BFDAuthSequenceNumber(1), BFDAuthData(s2)}
	fold b.Mem()
	ensures slices.AbsSlice_Bytes(s, 0, len(s))
	decreases
	outline(
	s := []byte{1, 2, 3}
	fold slices.AbsSlice_Bytes(s, 0, len(s))
	)
	ensures slices.AbsSlice_Bytes(s1, 0, len(s1))
	decreases
	outline(
	s1 := []byte{1, 2, 3}
	fold slices.AbsSlice_Bytes(s1, 0, len(s1))
	)
	var bf *BFD = &BFD{ BaseLayer{s, s1}, BFDVersion(1), BFDDiagnostic(1), BFDState(1), true, true, true, true, true, true, BFDDetectMultiplier(1), BFDDiscriminator(1), BFDDiscriminator(1), BFDTimeInterval(1), BFDTimeInterval(1), BFDTimeInterval(1), b }
	fold bf.Mem()
}

